{
    "extends": "astro/tsconfigs/strict",
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@components/*": [
                "src/components/*"
            ],
            "@images/*": [
                "src/images/*"
            ],
            "@content/*": [
                "src/content/*"
            ],
            "@layouts/*": [
                "src/layouts/*"
            ],
            "@styles/*": [
                "src/styles/*"
            ]
        }
    }
}